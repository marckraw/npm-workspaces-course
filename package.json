{
  "name": "@mrck-labs/root",
  "private": true,
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:vite-react-app": "npm run dev -w @mrck-labs/vite-react-app",
    "dev:utils": "npm run dev -w @mrck-labs/utils",
    "dev:anton-sdk": "npm run dev -w @mrck-labs/anton-sdk",
    "dev:lib-a": "npm run dev -w @mrck-labs/lib-a",
    "dev": "concurrently \"npm run dev:lib-a\" \"npm run dev:utils\" \"npm run dev:vite-react-app\" ",
    "dev:anton:utils": "concurrently \"npm run dev:utils\" \"npm run dev:anton-sdk\"",
    "build:utils": "npm run build -w @mrck-labs/utils",
    "build:lib-a": "npm run build -w @mrck-labs/lib-a",
    "build": "npm run build:lib-a && npm run build:utils",
    "release:lib-a": "npm run release -w @mrck-labs/lib-a",
    "release:utils": "npm run release -w @mrck-labs/utils",
    "release": "npm run release:lib-a && npm run release:utils"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
